package day4homework2;

import Abstracts.BaseCustomerManager;
import Adapters.MernisServiceAdapter;
import Concrete.NeroCustomerManager;
import Concrete.StarbucksCustomerManager;
import Entities.Customer;

public class Main {

	public static void main(String[] args) {
		
		BaseCustomerManager customerManager= new NeroCustomerManager();
		customerManager.Save(new Customer(1, "Berna", "Kahraman", 1997, "11111111111"));
		
		BaseCustomerManager customerManager2= new StarbucksCustomerManager(new MernisServiceAdapter());
		customerManager2.Save(new Customer(1, "Berna", "Kahraman", 1997, "11111111111"));
		
	}

}
